{
  "name": "june md",
  "description": "A WhatsApp bot based on Baileysjs library (Ts+Js) with Multi Device support",
  "repository": "https://github.com/Vinpink2/june-md",
  "logo": "",
  "keywords": ["nodejs", "bot", "whatsapp bot", "whatsapp automation", "multi device"],
  "success_url": "/",
  "stack": "heroku-24",
  "env": {
    "OWNER_NAME": {
      "description": "Enter your owner name here.",
      "value": "supreme",
      "required": true
    },
    "BOT_PREFIX": {
      "description": "Put your bot prefix of desire here (e.g., .)",
      "value": ".",
      "required": false
    },
    "SESSION_ID": {
      "description": "Put your session ID here (should start with JUNE-MD:~)",
      "value": "",
      "required": false
    }
  },
  "addons": [
    {
      "plan": "heroku-postgresql",
      "description": "Free PostgreSQL database for storing bot data."
    }
  ],
  "buildpacks": [
    {
      "url": "heroku/nodejs",
      "description": "Node.js runtime for running the bot."
    },
    {
      "url": "https://github.com/clhuang/heroku-buildpack-webp-binaries.git",
      "description": "WebP support for image processing."
    },
    {
      "url": "https://github.com/jonathanong/heroku-buildpack-ffmpeg-latest",
      "description": "FFmpeg for audio/video processing."
    }
  ],
  "formation": {
    "worker": {
      "quantity": 1,
      "size": "basic"
    }
  }
}
